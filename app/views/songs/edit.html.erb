<%= form_for(@song) do |f| %> 
    <label>Song Title:</label><br>
    <%= f.text_field :title %><br>

    <label>Song Released?:</label><br>
    <%= f.check_box :released %> <br>

    <label>Input Song release Year:</label><br>
    <%= f.text_field :release_year %><br>

    
    <label>Input Song Genre:</label><br>
    <%= f.text_field :genre %><br>
    
    <label>Input Song's Artist name:</label><br>
    <%= f.text_field :artist_name %><br>

    <%= f.submit %>
    <% end %>